{
  "name": "vite-plugin-el-form-submit-prevent",
  "version": "1.0.1",
  "description": "A Vite plugin that automatically adds @submit.native.prevent to el-form tags in Vue files",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run test:run",
    "lint": "eslint index.js test/**/*.js",
    "lint:fix": "eslint index.js test/**/*.js --fix"
  },
  "keywords": [
    "vite",
    "plugin",
    "element-ui",
    "el-form",
    "vue",
    "form",
    "submit",
    "prevent"
  ],
  "author": "Zayn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zayn8888888/vite-plugin-el-form-submit-prevent.git"
  },
  "bugs": {
    "url": "https://github.com/zayn8888888/vite-plugin-el-form-submit-prevent/issues"
  },
  "homepage": "https://github.com/zayn8888888/vite-plugin-el-form-submit-prevent#readme",
  "files": [
    "index.js",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "picomatch": "^4.0.2"
  }
}
